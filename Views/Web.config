<?xml version="1.0"?>
<configuration>
	<configSections>
		<sectionGroup name="system.web.webPages.razor" type="System.Web.WebPages.Razor.Configuration.RazorWebSectionGroup, System.Web.WebPages.Razor, Version=3.0.0.0, Culture=neutral, PublicKeyToken=b02f9f7f11d50a3a">
			<section name="host" type="System.Web.WebPages.Razor.Configuration.HostSection, System.Web.WebPages.Razor, Version=3.0.0.0, Culture=neutral, PublicKeyToken=b02f9f7f11d50a3a" requirePermission="false" />
			<section name="pages" type="System.Web.WebPages.Razor.Configuration.RazorPagesSection, System.Web.WebPages.Razor, Version=3.0.0.0, Culture=neutral, PublicKeyToken=b02f9f7f11d50a3a" requirePermission="false" />
		</sectionGroup>
	</configSections>

	<system.web.webPages.razor>
		<host factoryType="System.Web.Mvc.MvcWebRazorHostFactory, System.Web.Mvc, Version=5.2.7.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
		<pages pageBaseType="System.Web.Mvc.WebViewPage">
			<namespaces>
				<add namespace="System.Web.Mvc" />
				<add namespace="System.Web.Mvc.Ajax" />
				<add namespace="System.Web.Mvc.Html" />
				<add namespace="System.Web.Optimization"/>
				<add namespace="KindergartenSystem.Models" />
				<add namespace="KindergartenSystem.ViewModels" />
			</namespaces>
		</pages>
	</system.web.webPages.razor>

	<appSettings>
		<add key="webpages:Enabled" value="false" />
	</appSettings>

	<system.web>
		<httpHandlers>
			<add path="*" verb="*" type="System.Web.HttpNotFoundHandler"/>
			<add path="*.css" verb="GET" type="System.Web.Handlers.StaticFileHandler" />
			<add path="*.js" verb="GET" type="System.Web.Handlers.StaticFileHandler" />
			<add path="*.png" verb="GET" type="System.Web.Handlers.StaticFileHandler" />
			<add path="*.jpg" verb="GET" type="System.Web.Handlers.StaticFileHandler" />
			<add path="*.jpeg" verb="GET" type="System.Web.Handlers.StaticFileHandler" />
			<add path="*.gif" verb="GET" type="System.Web.Handlers.StaticFileHandler" />
		</httpHandlers>
		<pages validateRequest="false" />
		<httpRuntime targetFramework="4.7.2" />
	</system.web>
</configuration>